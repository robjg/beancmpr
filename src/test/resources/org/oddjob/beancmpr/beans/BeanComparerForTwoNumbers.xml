<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<oddjob>
    <job>
        <sequential>
            <jobs>
                <beancmpr:compare2 id="compare" xmlns:beancmpr="oddjob:beancmpr">
                    <inX>
                        <convert>
                            <to>
                                <class name="double"/>
                            </to>
                            <value>
                                <value value="5.2"/>
                            </value>
                        </convert>
                    </inX>
                    <inY>
                        <convert>
                            <to>
                                <class name="double"/>
                            </to>
                            <value>
                                <value value="5.3"/>
                            </value>
                        </convert>
                    </inY>
                    <comparer>
                        <beancmpr:bean-comparer>
                            <comparersByType>
                                <beancmpr:comparers-list>
                                    <specialisations>
                                        <beancmpr:numeric-comparer deltaTolerance="0.1" key="double"/>
                                    </specialisations>
                                </beancmpr:comparers-list>
                            </comparersByType>
                        </beancmpr:bean-comparer>
                    </comparer>
                    <results>
                        <beancmpr:bean-results>
                            <out>
                                <list/>
                            </out>
                        </beancmpr:bean-results>
                    </results>
                </beancmpr:compare2>
                <bean-report>
                    <beans>
                        <value value="${compare.results.out}"/>
                    </beans>
                </bean-report>
            </jobs>
        </sequential>
    </job>
</oddjob>
